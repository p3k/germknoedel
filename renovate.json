{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "npm:unpublishSafe",
    ":automergeMajor",
    ":disableRateLimiting",
    ":pinVersions",
    ":semanticCommitsDisabled"
